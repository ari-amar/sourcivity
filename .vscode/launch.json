	"version": "0.2.0",
	"configurations": [
		
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Python File from app",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
            "cwd": "${workspaceFolder}/app/api" 
		},
		{
			"name": "Python FastAPI",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/app/api/main.py",
			"console": "integratedTerminal",
            "cwd": "${workspaceFolder}/app/api" 
		}
	]
}